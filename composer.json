{
    "name": "mlverify/php-sdk",
    "description": "PHP Library from SwaggerHub, for use with MLVerify.com",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api",
        "aml",
        "kyc",
        "mlverify"
    ],
    "homepage": "https://mlverify.com",
    "license": "proprietary",
    "authors": [
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "MLVerify\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "MLVerify\\Client\\" : "test/" }
    }
}
